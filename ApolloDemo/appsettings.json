{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "Apollo": {
    "AppId": "apollo-dotnet-rpc",
    "MetaServer": "http://k8s-apollo-service",
    "ConfigServer": [ "http://k8s-apollo-service" ],
    "Namespaces": [ "application", "dotnet.rpc" ]
  }
}
